<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="struts2" extends="struts-default">

        <action name="stu_login" class="actions.stu_action" method = "execute">
            <result name="success">/navigationBar.html</result>
            <result name="fail">/login_error.jsp</result>
        </action>

        <!--管理员登录-->
        <action name="administratorLogin" class="actions.AdministratorLoginAction" method = "execute">
            <result name="success">/admSuccess.jsp</result>
            <result name="error">/admError.jsp</result>
        </action>

    </package>

</struts>
